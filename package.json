{
  "name": "open-course",
  "version": "1.0.0",
  "description": "OpenSource learning ebook",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm i && node scripts/dev.js",
    "dev:home": "pnpm --dir packages/home dev",
    "dev:nodejs": "pnpm --dir packages/nodejs docs:dev",
    "dev:interview": "pnpm --dir packages/interview docs:dev",
    "dev:algorithm": "pnpm --dir packages/algorithm docs:dev",
    "dev:linux": "pnpm --dir packages/linux docs:dev",
    "dev:qiankun": "pnpm --dir packages/qiankun docs:dev",
    "dev:react": "pnpm --dir packages/react start",
    "build:home": "pnpm --dir packages/home build",
    "build:nodejs": "pnpm --dir packages/nodejs docs:build",
    "build:interview": "pnpm --dir packages/interview docs:build",
    "build:algorithm": "pnpm --dir packages/algorithm docs:build",
    "build:linux": "pnpm --dir packages/linux docs:build",
    "build:qiankun": "pnpm --dir packages/qiankun docs:build",
    "build:react": "pnpm --dir packages/react build",
    "build:all": "node scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronlamz/open-course.git"
  },
  "keywords": [
    "ebook"
  ],
  "author": "aaronlamz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaronlamz/open-course/issues"
  },
  "homepage": "https://github.com/aaronlamz/open-course#readme",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "inquirer": "^8.1.0"
  }
}
